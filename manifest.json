{
  "manifest_version": 2,
  "name": "Comments first on IS.fi and HS.fi",
  "version": "1.1",
  "description": "Shows you the most important content first",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.is.fi/*", "https://www.hs.fi/*"],
      "js": ["dist/comments.js"]
    }
  ]
}
